{
  "name": "schlump",
  "version": "0.5.2",
  "description": "A static site generator utilizing reactjs",
  "bin": {
    "schlump": "./cli.js"
  },
  "main": "./index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "commitmsg": "conventional-changelog-lint -e",
    "release": "standard-version --no-verify",
    "postrelease": "git push --follow-tags origin master && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sinnerschrader/schlump.git"
  },
  "keywords": [
    "staticsite",
    "site",
    "reactjs",
    "github",
    "pages",
    "gh",
    "pages",
    "gh-pages"
  ],
  "engines": {
    "node": ">=6"
  },
  "author": "Markus Wolf",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sinnerschrader/schlump/issues"
  },
  "homepage": "https://github.com/sinnerschrader/schlump#readme",
  "dependencies": {
    "babel-core": "^6.14.0",
    "camelcase": "^3.0.0",
    "globby": "^6.0.0",
    "gray-matter": "^2.0.2",
    "meow": "^3.7.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "sander": "^0.5.1",
    "semver": "^5.3.0",
    "upper-case-first": "^1.1.2"
  },
  "devDependencies": {
    "conventional-changelog-lint": "^1.0.1",
    "cz-customizable": "^4.0.0",
    "husky": "^0.11.8",
    "standard-version": "^2.4.0",
    "xo": "^0.16.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./.cz-config.js"
    }
  }
}
